    </style>

    <!-- Labels -->

    <style name="TextAppearance.AgentPro.LabelLarge" parent="TextAppearance.Material3.LabelLarge">
        <item name="fontFamily">@font/outfit_bold</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="TextAppearance.AgentPro.LabelMedium" parent="TextAppearance.Material3.LabelMedium">
        <item name="fontFamily">@font/outfit_regular</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">12sp</item>
