    <style name="TextAppearance.AgentPro.TitleSmall" parent="TextAppearance.Material3.TitleSmall">
        <item name="fontFamily">@font/outfit_light</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
    </style>

    <!-- Bodies -->

    <style name="TextAppearance.AgentPro.BodyLarge" parent="TextAppearance.Material3.BodyLarge">
        <item name="fontFamily">@font/outfit_bold</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="TextAppearance.AgentPro.BodyMedium" parent="TextAppearance.Material3.BodyMedium">
        <item name="fontFamily">@font/outfit_regular</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
