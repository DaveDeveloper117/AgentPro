<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.AgentPro" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="colorPrimary">@color/md_theme_light_primary</item>
        <item name="colorOnPrimary">@color/md_theme_light_onPrimary</item>
        <item name="colorPrimaryContainer">@color/md_theme_light_primaryContainer</item>
        <item name="colorOnPrimaryContainer">@color/md_theme_light_onPrimaryContainer</item>
        <item name="colorSecondary">@color/md_theme_light_secondary</item>
        <item name="colorOnSecondary">@color/md_theme_light_onSecondary</item>
        <item name="colorSecondaryContainer">@color/md_theme_light_secondaryContainer</item>
        <item name="colorOnSecondaryContainer">@color/md_theme_light_onSecondaryContainer</item>
        <item name="colorTertiary">@color/md_theme_light_tertiary</item>
        <item name="colorOnTertiary">@color/md_theme_light_onTertiary</item>
        <item name="colorTertiaryContainer">@color/md_theme_light_tertiaryContainer</item>
        <item name="colorOnTertiaryContainer">@color/md_theme_light_onTertiaryContainer</item>
        <item name="colorError">@color/md_theme_light_error</item>
        <item name="colorErrorContainer">@color/md_theme_light_errorContainer</item>
        <item name="colorOnError">@color/md_theme_light_onError</item>
        <item name="colorOnErrorContainer">@color/md_theme_light_onErrorContainer</item>
        <item name="android:colorBackground">@color/md_theme_light_background</item>
        <item name="colorOnBackground">@color/md_theme_light_onBackground</item>
        <item name="colorSurface">@color/md_theme_light_surface</item>
        <item name="colorOnSurface">@color/md_theme_light_onSurface</item>
        <item name="colorSurfaceVariant">@color/md_theme_light_surfaceVariant</item>
        <item name="colorOnSurfaceVariant">@color/md_theme_light_onSurfaceVariant</item>
        <item name="colorOutline">@color/md_theme_light_outline</item>
        <item name="colorOnSurfaceInverse">@color/md_theme_light_inverseOnSurface</item>
        <item name="colorSurfaceInverse">@color/md_theme_light_inverseSurface</item>
        <item name="colorPrimaryInverse">@color/md_theme_light_inversePrimary</item>
        <!--Display-->
        <item name="textAppearanceDisplayLarge">@style/TextAppearance.AgentPro.DisplayLarge</item>
        <item name="textAppearanceDisplayMedium">@style/TextAppearance.AgentPro.DisplayMedium</item>
        <item name="textAppearanceDisplaySmall">@style/TextAppearance.AgentPro.DisplaySmall</item>
        <!--Headline-->
        <item name="textAppearanceHeadlineLarge">@style/TextAppearance.AgentPro.HeadlineLarge</item>
        <item name="textAppearanceHeadlineMedium">@style/TextAppearance.AgentPro.HeadlineMedium</item>
        <item name="textAppearanceHeadlineSmall">@style/TextAppearance.AgentPro.HeadlineSmall</item>
        <!--Title-->
        <item name="textAppearanceTitleLarge">@style/TextAppearance.AgentPro.TitleLarge</item>
        <item name="textAppearanceTitleMedium">@style/TextAppearance.AgentPro.TitleMedium</item>
        <item name="textAppearanceTitleSmall">@style/TextAppearance.AgentPro.TitleSmall</item>
        <!--Body-->
        <item name="textAppearanceBodyLarge">@style/TextAppearance.AgentPro.BodyLarge</item>
        <item name="textAppearanceBodyMedium">@style/TextAppearance.AgentPro.BodyMedium</item>
        <item name="textAppearanceBodySmall">@style/TextAppearance.AgentPro.BodySmall</item>
        <!--Card-->
        <item name="materialCardViewStyle">@style/Widget.AgentPro.CardView</item>
        <!--FAB-->
        <item name="floatingActionButtonStyle">@style/Widget.AgentPro.FloatingActionButton</item>
    </style>

    <!--FABs-->
    <style name="Widget.AgentPro.FloatingActionButton" parent="Widget.Material3.FloatingActionButton.Primary">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.FloatingActionButton</item>
    </style>

    <style name="ThemeOverlay.App.FloatingActionButton" parent="">
        <item name="colorContainer">@color/md_theme_light_secondaryContainer</item>
        <item name="colorOnContainer">@color/md_theme_light_onSecondaryContainer</item>
    </style>

    <!--Card-->
    <style name="Widget.AgentPro.CardView" parent="Widget.Material3.CardView.Elevated">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.Card</item>
        <item name="shapeAppearance">@style/ShapeAppearance.App.MediumComponent</item>
    </style>

    <style name="ThemeOverlay.App.Card" parent="">
        <item name="colorSecondary">@color/md_theme_light_secondary</item>
        <item name="colorSurface">@color/md_theme_light_surface</item>
        <item name="colorOnSurfaceVariant">@color/md_theme_light_onSurfaceVariant</item>
    </style>

    <style name="ShapeAppearance.App.MediumComponent" parent="ShapeAppearance.Material3.MediumComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">30dp</item>
    </style>

    <!-- Displays -->
    <style name="TextAppearance.AgentPro.DisplayLarge" parent="TextAppearance.Material3.DisplayLarge">
        <item name="fontFamily">@font/outfit_bold</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">57sp</item>
    </style>
    <style name="TextAppearance.AgentPro.DisplayMedium" parent="TextAppearance.Material3.DisplayMedium">
        <item name="fontFamily">@font/outfit_regular</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">45sp</item>
    </style>
    <style name="TextAppearance.AgentPro.DisplaySmall" parent="TextAppearance.Material3.DisplaySmall">
        <item name="fontFamily">@font/outfit_light</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">36sp</item>
    </style>

    <!-- Headlines -->

    <style name="TextAppearance.AgentPro.HeadlineLarge" parent="TextAppearance.Material3.HeadlineLarge">
        <item name="fontFamily">@font/outfit_bold</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">32sp</item>
    </style>
    <style name="TextAppearance.AgentPro.HeadlineMedium" parent="TextAppearance.Material3.HeadlineMedium">
        <item name="fontFamily">@font/outfit_regular</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">28sp</item>
