    <style name="TextAppearance.AgentPro.HeadlineSmall" parent="TextAppearance.Material3.HeadlineSmall">
        <item name="fontFamily">@font/outfit_light</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">24sp</item>
    </style>

    <!-- Titles -->

    <style name="TextAppearance.AgentPro.TitleLarge" parent="TextAppearance.Material3.TitleLarge">
        <item name="fontFamily">@font/outfit_bold</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">22sp</item>
    </style>
    <style name="TextAppearance.AgentPro.TitleMedium" parent="TextAppearance.Material3.TitleMedium">
        <item name="fontFamily">@font/outfit_regular</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
